import java.util.*;
public class Main
{
    public int Factorial(int x){
        if(x==1)
        return 1;
        else
        return x*Factorial(x-1);
    }
	public static void main(String[] args) {
		Main obj=new Main();
		Scanner sc =new Scanner(System.in);
		int n=sc.nextInt();
		System.out.println(obj.Factorial(n));
	}
}
