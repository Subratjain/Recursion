import java.util.*;
public class Main
{  
static int a=0,b=1,c=0;
    static void fib(int n){
        if(n>0){
        c=a+b;
        a=b;
        b=c;
        System.out.println(c);
        fib(n-1);
        }
    }
	public static void main(String[] args) {
		Main obj=new Main();
		int count=10;
		System.out.println(0);
		System.out.println(1);
		
		obj.fib(count-2);
	}
}
